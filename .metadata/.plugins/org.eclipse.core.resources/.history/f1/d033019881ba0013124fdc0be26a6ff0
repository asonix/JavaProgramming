import java.io.*;
import java.util.*;
public class project17 {
	public static void main(String args[]) {
		System.out.print("Enter something like 8 + 33 + 1,345 +137 : ");
		Scanner q = new Scanner(System.in);
		String e = q.nextLine();
		Scanner equation = new Scanner(e);
		equation.useDelimiter("");
		int total = equation.nextInt();
		
		while(equation.hasNext()) {
			String n = equation.next();
			if(n == "+") {
				int m = equation.nextInt();
				total += m;
				System.out.print("m = ");
				System.out.print(m);
				System.out.println("+");
			}
			else if(n == "-") {
				int m = equation.nextInt();
				total -= m;
				System.out.println("-");
			}
		}
		System.out.println("total:");
		System.out.println(total);
	}
}
